    {%- when 'article' -%}
      {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
{%- if disabled_by_yoast_seo -%}
<meta name="keywords" content="{{article.title | escape}}, {{blog.title | escape}}, {{shop_name_escape}}, {{shop_domain}}"/>
